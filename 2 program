//  single line command

#include <stdio.h>

int main()
{
    int a,b,sum;
    printf("enter two numbers");//output the hello orld to the console
    scanf("%d %d",&a, &b);
    sum=a+b;
    printf("sum = %d ",sum);
    return 0;//this is return 0 
}
